      <DataSource1 Ref="4" type="Stimulsoft.Report.Dictionary.StiSqlSource" isKey="true">
        <Alias>DataSource1</Alias>
        <Columns isList="true" count="13">
          <value>kc_area_code,System.String</value>
          <value>kc_buy_date,System.DateTime</value>
          <value>kc_case_cnt,System.Int32</value>
          <value>kc_case_no,System.String</value>
          <value>kc_cust_nameu,System.String</value>
          <value>kc_del_type,System.String</value>
          <value>kc_give_amt,System.Int32</value>
          <value>kc_issu_code,System.String</value>
          <value>kc_movable_stat,System.String</value>
          <value>kc_pay_amt,System.Int32</value>
          <value>kc_proc_fee,System.Int32</value>
          <value>kc_remit_amt,System.Int32</value>
          <value>kc_remit_date,System.DateTime</value>
        </Columns>
        <CommandTimeout>180</CommandTimeout>
        <Dictionary isRef="1" />
        <Name>DataSource1</Name>
        <NameInSource>Dys</NameInSource>
        <Parameters isList="true" count="2">
          <value>wk_strt_date,,4,0</value>
          <value>wk_stop_date,,4,0</value>
        </Parameters>
        <SqlCommand>select c.kc_area_code,c.kc_issu_code,c.kc_buy_date,c.kc_case_no,kc_cust_nameu,(select top 1 kc_movable_stat from kcsd.kc_movable where kc_cp_no = c.kc_cp_no order by kc_item_no desc) as kc_movable_stat,
	CASE WHEN kc_prod_type='04' THEN (CASE WHEN substring(c.kc_bank_code,1,3) = '803' THEN 0 ELSE 30 END) ELSE (CASE WHEN substring(a.kc_bank_code,1,3) = '803' THEN 0 ELSE 30 END) END AS kc_proc_fee,
	CASE WHEN d.kc_del_type = 'S' THEN 1500 ELSE 0 END AS kc_case_cnt,
	c.kc_give_amt,r.kc_remit_amt,r.kc_remit_date,d.kc_del_type,CASE WHEN r.kc_remit_date is not null THEN r.kc_remit_amt ELSE 0 END AS kc_pay_amt
from kcsd.kc_remittance r 
left join kcsd.kc_customerloan c on c.kc_case_no = r.kc_case_no
left join kcsd.kc_caragent a on a.kc_agent_code = c.kc_comp_code
left join kcsd.kc_customerdelete d on d.kc_case_no = r.kc_case_no
where 
r.kc_remit_date  between @wk_strt_date and @wk_stop_date</SqlCommand>
      </DataSource1>
