      <DataSource1 Ref="4" type="Stimulsoft.Report.Dictionary.StiSqlSource" isKey="true">
        <Alias>DataSource1</Alias>
        <Columns isList="true" count="13">
          <value>kc_break_sum,System.Int32</value>
          <value>kc_case_no,System.String</value>
          <value>kc_cust_nameu,System.String</value>
          <value>kc_intr_sum,System.Int32</value>
          <value>kc_invo_date,System.DateTime</value>
          <value>kc_invo_sum2,System.Int32</value>
          <value>kc_issu_code,System.String</value>
          <value>kc_local_sum,System.Int32</value>
          <value>kc_pay_desc,System.String</value>
          <value>kc_pay_sum,System.Int32</value>
          <value>kc_pay_type,System.String</value>
          <value>kc_proc_sum,System.Int32</value>
          <value>kc_pvpay_sum2,System.Int32</value>
        </Columns>
        <CommandTimeout>180</CommandTimeout>
        <Dictionary isRef="1" />
        <Name>DataSource1</Name>
        <NameInSource>Dys</NameInSource>
        <Parameters isList="true" count="3">
          <value>wk_strt_date,,4,0</value>
          <value>wk_stop_date,,4,0</value>
          <value>wk_issu_code,,22,2</value>
        </Parameters>
        <SqlCommand>SELECT l.kc_case_no, c.kc_issu_code, c.kc_cust_nameu, l.kc_invo_date, l.kc_pay_type,p.kc_pay_desc,
	Sum(l.kc_pay_fee) AS kc_pay_sum,
	Sum(kc_break_fee) AS kc_break_sum,
	Sum(l.kc_intr_fee) AS kc_intr_sum,
	Sum(l.kc_invo_amt2) AS kc_invo_sum2,
	Sum([kc_invo_amt2]-[kc_intr_fee]) AS kc_local_sum,
	Sum(l.kc_proc_fee) AS kc_proc_sum,
	Sum(l.kc_pvpay_amt2) AS kc_pvpay_sum2
	FROM kcsd.kc_loanpayment l
INNER JOIN kcsd.kc_customerloan c ON l.kc_case_no = c.kc_case_no
LEFT JOIN kcsd.kct_paytype p ON l.kc_pay_type = p.kc_pay_type
WHERE c.kc_loan_stat Not In ('X','Y','Z') AND
c.kc_issu_code = @wk_issu_code AND
c.kc_prod_type = '08' AND --勞務件
l.kc_invo_date Between @wk_strt_date And @wk_stop_date AND
(c.kc_idle_date Is Null Or c.kc_idle_date &gt;= @wk_stop_date) 
GROUP BY l.kc_case_no, c.kc_issu_code, c.kc_cust_nameu, l.kc_invo_date, l.kc_pay_type, p.kc_pay_desc;</SqlCommand>
      </DataSource1>
