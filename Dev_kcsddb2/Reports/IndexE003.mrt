      <DataSource1 Ref="3" type="Stimulsoft.Report.Dictionary.StiSqlSource" isKey="true">
        <Alias>DataSource1</Alias>
        <Columns isList="true" count="7">
          <value>kc_sales_code,System.String</value>
          <value>cnt,System.Int32</value>
          <value>cp_cnt,System.Int32</value>
          <value>cnt_14dudu,System.Int32</value>
          <value>pr,System.String</value>
          <value>new,System.Int32</value>
          <value>all,System.Int32</value>
        </Columns>
        <CommandTimeout>180</CommandTimeout>
        <Dictionary isRef="1" />
        <Key>3aede5974671408b87cf3f0dec79759e</Key>
        <Name>DataSource1</Name>
        <NameInSource>Dys</NameInSource>
        <Parameters isList="true" count="3">
          <value>wk_strt_date,,22,10</value>
          <value>wk_stop_date,,22,10</value>
          <value>wk_list_user,,22,4</value>
        </Parameters>
        <SqlCommand>SELECT kc_sales_code, SUM(cp_cnt) 'cp_cnt', SUM(cnt_14dudu)'cnt_14dudu', SUM(cnt) 'cnt', 
	   CASE WHEN SUM(cp_cnt)=0 THEN '0.00%' ELSE FORMAT((CAST(SUM(cnt) AS DECIMAL(18, 4)) * 100.0) / SUM(cp_cnt),'N2') + '%' END 'pr',
	   SUM([new])'new', SUM([all])'all'
FROM 
(
	SELECT kc_sales_code, ( SELECT count(*) FROM kcsd.kc_cpdata cu3 WHERE cu3.kc_sales_code = cu.kc_sales_code AND cu3.kc_cp_date BETWEEN @wk_strt_date AND @wk_stop_date)'cp_cnt', 0'cnt_14dudu', count(*)'cnt', 
		   0'new', 0'all'
	FROM kcsd.kc_customerloan cu
	LEFT JOIN kcsd.Delegate D ON cu.kc_sales_code = D.DelegateUser
 	WHERE kc_loan_stat not in ('X','Y','Z') 
	  AND cu.kc_prod_type in ('04','11') 
	  AND cu.kc_area_code = ('00')  
	  AND kc_buy_date BETWEEN @wk_strt_date AND @wk_stop_date
      AND ( @wk_list_user = '' OR kc_sales_code = @wk_list_user ) 
	GROUP BY kc_sales_code

	UNION

	SELECT kc_introduce_code,  0'cp_cnt', COUNT(*)'cnt_14dudu', 0'cnt', 
		   0'new', 0'all'
	FROM kcsd.kc_cpdata cp
	LEFT JOIN [Zephyr.Sys].[dbo].[sys_user] u ON cp.kc_introduce_code = u.UserSeq
	WHERE kc_prod_type = '14' 
	  AND kc_apply_stat = 'P'
	  AND kc_cp_date BETWEEN @wk_strt_date AND @wk_stop_date
	  AND kc_introduce_code IS NOT NULL 
	  AND u.UserSeq IS NOT NULL 
      AND ( @wk_list_user = '' OR kc_introduce_code = @wk_list_user ) 
	GROUP BY kc_introduce_code

	UNION 

	SELECT UserSeq, 0'cp_cnt', 0'cnt_14dudu', 0'cnt',  
		   COUNT(*)'new', 0'all'
	FROM kcsd.kc_marketer M
	INNER JOIN kcsd.Delegate D ON M.kc_marketer_code = D.DelegateCode
	INNER JOIN [Zephyr.Sys].dbo.sys_user U ON D.DelegateUser=U.UserSeq
	WHERE kc_strt_date BETWEEN @wk_strt_date AND @wk_stop_date
      AND ( @wk_list_user = '' OR UserSeq = @wk_list_user ) 
	GROUP BY UserSeq

	UNION 

	SELECT UserSeq, 0'cp_cnt', 0'cnt_14dudu', 0'cnt',  
           0'new', COUNT(*)'all'
	FROM kcsd.kc_marketer M
	INNER JOIN kcsd.Delegate D ON M.kc_marketer_code = D.DelegateCode
	INNER JOIN [Zephyr.Sys].dbo.sys_user U ON D.DelegateUser=U.UserSeq
	WHERE kc_stop_date IS NULL
      AND ( @wk_list_user = '' OR UserSeq = @wk_list_user ) 
	GROUP BY UserSeq

) as xx
GROUP BY kc_sales_code</SqlCommand>
      </DataSource1>
