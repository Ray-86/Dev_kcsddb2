        <Parameters isList="true" count="8">
          <value>wk_strt_date,,4,0</value>
          <value>wk_stop_date,,4,0</value>
          <value>wk_stop_date2,,4,0</value>
          <value>wk_strt_date2,,4,0</value>
          <value>wk_prod_type,,22,50</value>
          <value>wk_new_flag,,23,0</value>
          <value>wk_area_code,,23,0</value>
          <value>wk_prod_type_desc,,22,50</value>
        </Parameters>
        <SqlCommand>SELECT
	'01'as data_type,f.kc_over_count,f.kc_over_amt,A.kc_loan_perd,A.kc_perd_fee,(SELECT ISNULL(SUM(kc_pay_fee), 0) FROM kcsd.kc_loanpayment WHERE kc_case_no = A.kc_case_no and kc_pay_date &lt;= @wk_stop_date2) as kc_pay_fee,
p.kc_cfm_user,p.kc_give_amt,
CASE WHEN (select kc_cons_rate from kcsd.kc_cpdata where kc_cp_no = A.kc_cp_no) &lt; 0.08 THEN 'A 8%以下' 
WHEN (select kc_cons_rate from kcsd.kc_cpdata where kc_cp_no = A.kc_cp_no) &gt;= 0.08 and (select kc_cons_rate from kcsd.kc_cpdata where kc_cp_no = A.kc_cp_no) &lt; 0.15 THEN 'B 8%-15%' 
WHEN (select kc_cons_rate from kcsd.kc_cpdata where kc_cp_no = A.kc_cp_no) &gt;= 0.15 and (select kc_cons_rate from kcsd.kc_cpdata where kc_cp_no = A.kc_cp_no) &lt; 0.20 THEN 'C 15-20%' 
	 ELSE 'D 20%以上' END as kc_level_no,A.kc_prod_type,
SUM(A.kc_perd_fee * A.kc_loan_perd) AS wk_expt_allsum,
SUM(CASE WHEN A.kc_cred_chk = 0 THEN 0 ELSE A.kc_cred_fee END) AS wk_credit_fee,
SUM(ISNULL(A.kc_insu_sum,0)) AS wk_insu_fee,
SUM(ISNULL(A.kc_totm_fee,0)) AS wk_totm_fee,
SUM(ISNULL(A.kc_aux_amt,0)*180) AS wk_aux_fee,
SUM(CASE WHEN A.kc_give_amt IS NULL THEN CASE WHEN A.kc_coll_amt IS NULL THEN A.kc_loan_fee ELSE 0 END ELSE A.kc_give_amt END) AS wk_give_amt,
SUM(ISNULL(c.wk_pay_sum,0)) AS wk_pay_sum,
SUM(ISNULL(c.wk_intr_sum,0)) AS wk_intr_sum,
SUM(DATEDIFF(day,A.kc_buy_date,A.kc_strt_date)) AS wk_date_sum,
SUM(ISNULL(e.wk_expt_sum,0)) AS wk_expt_sum,
SUM(ISNULL(d.wk_pay_amt,0)) AS wk_pay_amt,
sum((((A.kc_loan_perd * A.kc_perd_fee)-(A.kc_give_amt + isnull(l.wk_insu_fee,0)))*12)) as a, sum((A.kc_loan_perd*(A.kc_give_amt+isnull(l.wk_insu_fee,0)))) as b,
(sum((((A.kc_loan_perd * A.kc_perd_fee)-(A.kc_give_amt + isnull(l.wk_insu_fee,0)))*12)))*1.0 / sum((A.kc_loan_perd*(A.kc_give_amt+isnull(l.wk_insu_fee,0)))) as avg_ans
FROM kcsd.kc_customerloan A left join kcsd.kc_cpdata p on p.kc_cp_no = A.kc_cp_no
left join [Zephyr.Sys].[dbo].[sys_user] u on p.kc_cfm_user = u.UserCode  
LEFT JOIN(select isnull(sum(kc_insu_fee),0) as wk_insu_fee , kc_case_no from kcsd.kc_insurance_list_insu GROUP BY kc_case_no)l on A.kc_case_no = l.kc_case_no
LEFT JOIN (
SELECT kc_case_no, Sum(kc_pay_fee) AS wk_pay_sum, Sum(kc_intr_fee) AS wk_intr_sum
	FROM kcsd.kc_loanpayment
	WHERE kc_pay_date Between @wk_strt_date2 And @wk_stop_date2
	--WHERE kc_pay_date Between '2019-10-01' And '2019-10-31'
GROUP BY kc_case_no
) c ON A.kc_case_no = c.kc_case_no
LEFT JOIN (
SELECT kc_case_no, Sum(kc_pay_fee) - Sum(kc_intr_fee) as wk_pay_amt
	FROM kcsd.kc_loanpayment
	WHERE kc_pay_date &lt;= @wk_stop_date2 and kc_expt_date &lt;= @wk_stop_date2
	--WHERE kc_pay_date &lt;='2019-10-31' and kc_expt_date &lt;='2019-10-31'
	GROUP BY kc_case_no
	) d ON A.kc_case_no = d.kc_case_no
LEFT JOIN (
SELECT kc_case_no, Sum(kc_expt_fee) AS wk_expt_sum
	FROM kcsd.kc_loanpayment
	WHERE kc_expt_date &lt;= @wk_stop_date2
	--WHERE kc_expt_date &lt;= '2019-10-31'
	and kc_perd_no &lt; 50
	GROUP BY kc_case_no
	) e ON A.kc_case_no = e.kc_case_no
LEFT JOIN (
SELECT kc_case_no,
	ISNULL((SELECT	DATEDIFF(day,MIN(kc_expt_date),DATEADD(day,1,@wk_stop_date2))/30 FROM kcsd.kc_loanpayment WHERE kc_case_no = c.kc_case_no AND (kc_pay_date &gt;= DATEADD(day,1,@wk_stop_date2) OR kc_pay_date IS NULL)),0) AS kc_over_count,
(SELECT ISNULL(SUM(kc_expt_fee), 0) FROM kcsd.kc_loanpayment WHERE kc_case_no = c.kc_case_no AND kc_expt_date &lt; DATEADD(day, 1, @wk_stop_date2 ) AND kc_perd_no &lt; 50)-(SELECT ISNULL(SUM(kc_pay_fee), 0) FROM kcsd.kc_loanpayment WHERE kc_case_no = c.kc_case_no AND kc_pay_date &lt;DATEADD(day, 1, @wk_stop_date2 ))  AS kc_over_amt
FROM	kcsd.kc_customerloan c
WHERE kc_buy_date BETWEEN @wk_strt_date  AND @wk_stop_date
and kc_loan_stat Not In ('Y','Z')
AND (@wk_prod_type = '' OR kc_prod_type = @wk_prod_type)
AND (@wk_new_flag = '' OR kc_new_flag = @wk_new_flag)
	) f ON A.kc_case_no = f.kc_case_no
WHERE kc_loan_stat Not In ('Y','Z') 
--AND A.kc_buy_date Between '2019-10-01' And '2019-10-31'
AND A.kc_buy_date Between @wk_strt_date And @wk_stop_date
AND (@wk_prod_type = '' OR A.kc_prod_type = @wk_prod_type)
AND (@wk_new_flag = '' OR A.kc_new_flag = @wk_new_flag)
AND p.kc_cfm_user is not null and p.kc_cfm_stat in ('P','D')
AND   (@wk_area_code = '' OR u.AreaCode = @wk_area_code)

GROUP BY A.kc_cp_no,p.kc_cfm_user,f.kc_over_count,f.kc_over_amt,A.kc_loan_perd,A.kc_perd_fee,A.kc_case_no ,p.kc_give_amt,A.kc_prod_type

UNION all

SELECT
	'00'as data_type,f.kc_over_count,f.kc_over_amt,A.kc_loan_perd,A.kc_perd_fee,(SELECT ISNULL(SUM(kc_pay_fee), 0) FROM kcsd.kc_loanpayment WHERE kc_case_no = A.kc_case_no and kc_pay_date &lt;= dateadd(dd,-day(@wk_stop_date2),@wk_stop_date2) ) as kc_pay_fee,
p.kc_cfm_user,p.kc_give_amt,
CASE WHEN (select kc_cons_rate from kcsd.kc_cpdata where kc_cp_no = A.kc_cp_no) &lt; 0.08 THEN 'A 8%以下' 
WHEN (select kc_cons_rate from kcsd.kc_cpdata where kc_cp_no = A.kc_cp_no) &gt;= 0.08 and (select kc_cons_rate from kcsd.kc_cpdata where kc_cp_no = A.kc_cp_no) &lt; 0.15 THEN 'B 8%-15%' 
WHEN (select kc_cons_rate from kcsd.kc_cpdata where kc_cp_no = A.kc_cp_no) &gt;= 0.15 and (select kc_cons_rate from kcsd.kc_cpdata where kc_cp_no = A.kc_cp_no) &lt; 0.20 THEN 'C 15-20%' 
	 ELSE 'D 20%以上' END as kc_level_no,A.kc_prod_type,
SUM(A.kc_perd_fee * A.kc_loan_perd) AS wk_expt_allsum,
'' AS wk_credit_fee,
'' AS wk_insu_fee,
'' AS wk_totm_fee,
'' AS wk_aux_fee,
'' AS wk_give_amt,
'' AS wk_pay_sum,
'' AS wk_intr_sum,
'' AS wk_date_sum,
SUM(ISNULL(e.wk_expt_sum,0)) AS wk_expt_sum,
SUM(ISNULL(d.wk_pay_amt,0)) AS wk_pay_amt,
'' as a,'' as b,
0 as avg_ans
FROM kcsd.kc_customerloan A left join kcsd.kc_cpdata p on p.kc_cp_no = A.kc_cp_no
left join [Zephyr.Sys].[dbo].[sys_user] u on p.kc_cfm_user = u.UserCode  
LEFT JOIN (
SELECT kc_case_no, Sum(kc_pay_fee) - Sum(kc_intr_fee) as wk_pay_amt
	FROM kcsd.kc_loanpayment
	WHERE kc_pay_date &lt;= dateadd(dd,-day(@wk_stop_date2),@wk_stop_date2)  and kc_expt_date &lt;= dateadd(dd,-day(@wk_stop_date2),@wk_stop_date2) 
	--WHERE kc_pay_date &lt;='2019-10-31' and kc_expt_date &lt;='2019-10-31'
	GROUP BY kc_case_no
	) d ON A.kc_case_no = d.kc_case_no
LEFT JOIN (
SELECT kc_case_no, Sum(kc_expt_fee) AS wk_expt_sum
	FROM kcsd.kc_loanpayment
	WHERE kc_expt_date &lt;= dateadd(dd,-day(@wk_stop_date2),@wk_stop_date2) 
	--WHERE kc_expt_date &lt;= '2019-10-31'
	and kc_perd_no &lt; 50
	GROUP BY kc_case_no
	) e ON A.kc_case_no = e.kc_case_no
LEFT JOIN (
SELECT kc_case_no,
	ISNULL((SELECT	DATEDIFF(day,MIN(kc_expt_date),DATEADD(day,1,dateadd(dd,-day(@wk_stop_date2),@wk_stop_date2) ))/30 FROM kcsd.kc_loanpayment WHERE kc_case_no = c.kc_case_no AND (kc_pay_date &gt;= DATEADD(day,1,dateadd(dd,-day(@wk_stop_date2),@wk_stop_date2) ) OR kc_pay_date IS NULL)),0) AS kc_over_count,
(SELECT ISNULL(SUM(kc_expt_fee), 0) FROM kcsd.kc_loanpayment WHERE kc_case_no = c.kc_case_no AND kc_expt_date &lt; DATEADD(day, 1, dateadd(dd,-day(@wk_stop_date2),@wk_stop_date2)  ) AND kc_perd_no &lt; 50)-(SELECT ISNULL(SUM(kc_pay_fee), 0) FROM kcsd.kc_loanpayment WHERE kc_case_no = c.kc_case_no AND kc_pay_date &lt;DATEADD(day, 1, dateadd(dd,-day(@wk_stop_date2),@wk_stop_date2)  ))  AS kc_over_amt
FROM	kcsd.kc_customerloan c
WHERE kc_buy_date BETWEEN dateadd(dd,-day(dateadd(month,-1,@wk_strt_date))+1,dateadd(month,-1,@wk_strt_date)) AND dateadd(dd,-day(@wk_stop_date),@wk_stop_date) 
and kc_loan_stat Not In ('Y','Z')
AND (@wk_prod_type = '' OR kc_prod_type IN (SELECT value FROM STRING_SPLIT(@wk_prod_type, ',')))
AND (@wk_new_flag = '' OR kc_new_flag = @wk_new_flag)
	) f ON A.kc_case_no = f.kc_case_no
WHERE kc_loan_stat Not In ('Y','Z') 
--AND A.kc_buy_date Between '2019-10-01' And '2019-10-31'
AND A.kc_buy_date Between dateadd(dd,-day(dateadd(month,-1,@wk_strt_date))+1,dateadd(month,-1,@wk_strt_date)) AND dateadd(dd,-day(@wk_stop_date),@wk_stop_date) 
AND (@wk_prod_type = '' OR A.kc_prod_type IN (SELECT value FROM STRING_SPLIT(@wk_prod_type, ',')))
AND (@wk_new_flag = '' OR A.kc_new_flag = @wk_new_flag)
AND p.kc_cfm_user is not null and p.kc_cfm_stat in ('P','D')
AND   (@wk_area_code = '' OR u.AreaCode = @wk_area_code)
GROUP BY A.kc_cp_no,p.kc_cfm_user,f.kc_over_count,f.kc_over_amt,A.kc_loan_perd,A.kc_perd_fee,A.kc_case_no,p.kc_give_amt,A.kc_prod_type




</SqlCommand>
