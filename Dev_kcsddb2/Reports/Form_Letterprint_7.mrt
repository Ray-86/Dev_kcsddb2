--select  @kc_case_no = '2022027'


if(@check_the_kc_item_no &gt; 0)  -- from 客戶資料查詢/催告
    begin  
        select A.kc_case_no,A.kc_law_code,A.kc_item_no,A.kc_law_date,B.kc_buy_date AS 建檔日期,B.kc_car_model AS 機型,B.kc_strt_date AS 起始日期,DatePart(DAY,B.kc_strt_date) AS 每月繳款日,B.kc_perd_fee AS 期付金額,
        B.kc_loan_perd AS 期數,
        B.kc_rema_amt AS 未繳餘額,L.ContactData2 AS 承辦電話2,CASE WHEN A.kc_perm_flag = 'Y' THEN B.kc_cust_nameu WHEN A.kc_curr_flag = 'Y' THEN B.kc_cust_nameu WHEN A.kc_comp_flag = 'Y' THEN B.kc_cust_nameu WHEN A.kc_perm_flag1 = 'Y' THEN B.kc_cust_name1u WHEN A.kc_curr_flag1 = 'Y' THEN B.kc_cust_name1u WHEN A.kc_comp_flag1 = 'Y' THEN B.kc_cust_name1u WHEN A.kc_perm_flag2 = 'Y' THEN B.kc_cust_name2u WHEN A.kc_curr_flag2 = 'Y' THEN B.kc_cust_name2u WHEN A.kc_comp_flag2 = 'Y' THEN B.kc_cust_name2u WHEN A.kc_bill_flag = 'Y' THEN B.kc_cust_nameu END AS 收件人,
        CASE WHEN A.kc_perm_flag = 'Y' THEN B.kc_id_no WHEN A.kc_curr_flag = 'Y' THEN B.kc_id_no WHEN A.kc_comp_flag = 'Y' THEN B.kc_id_no WHEN A.kc_perm_flag1 = 'Y' THEN B.kc_id_no1 WHEN A.kc_curr_flag1 = 'Y' THEN B.kc_id_no1 WHEN A.kc_comp_flag1 = 'Y' THEN B.kc_id_no1 WHEN A.kc_perm_flag2 = 'Y' THEN B.kc_id_no2 WHEN A.kc_curr_flag2 = 'Y' THEN B.kc_id_no2 WHEN A.kc_comp_flag2 = 'Y' THEN B.kc_id_no2 WHEN A.kc_bill_flag = 'Y' THEN B.kc_id_no END AS 收件人ID,
        CASE WHEN A.kc_perm_flag = 'Y' THEN B.kc_perm_zip WHEN A.kc_curr_flag = 'Y' THEN B.kc_curr_zip WHEN A.kc_comp_flag = 'Y' THEN B.kc_comp_zip WHEN A.kc_perm_flag1 = 'Y' THEN B.kc_perm_zip1 WHEN A.kc_curr_flag1 = 'Y' THEN B.kc_curr_zip1 WHEN A.kc_comp_flag1 = 'Y' THEN B.kc_comp_zip1 WHEN A.kc_perm_flag2 = 'Y' THEN B.kc_perm_zip2 WHEN A.kc_curr_flag2 = 'Y' THEN B.kc_curr_zip2 WHEN A.kc_comp_flag2 = 'Y' THEN B.kc_comp_zip2 WHEN A.kc_bill_flag = 'Y' THEN B.kc_zip_code END AS 收件郵遞區號,
        CASE WHEN A.kc_perm_flag = 'Y' THEN B.kc_perm_addr WHEN A.kc_curr_flag = 'Y' THEN B.kc_curr_addr WHEN A.kc_comp_flag = 'Y' THEN B.kc_comp_addr WHEN A.kc_perm_flag1 = 'Y' THEN B.kc_perm_addr1 WHEN A.kc_curr_flag1 = 'Y' THEN B.kc_curr_addr1 WHEN A.kc_comp_flag1 = 'Y' THEN B.kc_comp_addr1 WHEN A.kc_perm_flag2 = 'Y' THEN B.kc_perm_addr2 WHEN A.kc_curr_flag2 = 'Y' THEN B.kc_curr_addr2 WHEN A.kc_comp_flag2 = 'Y' THEN B.kc_comp_addr2 WHEN A.kc_bill_flag = 'Y' THEN B.kc_bill_addr END AS 收件地址 from [kcsd].[kc_lawstatus] as A,  
        [kcsd].[kc_customerloan] as B 
        LEFT JOIN [kcsd].[Delegate] L ON B.kc_pusher_code = L.DelegateCode 
        where  
        A.kc_case_no = @kc_case_no
        and A.kc_case_no = B.kc_case_no
        --and kc_item_no=@kc_item_no
        and kc_law_code = '7' 
        and kc_item_no=@check_the_kc_item_no
        --A.kc_case_no = '2022027'
        --and kc_law_code = 'B' 
        --and kc_item_no=1
        order by kc_item_no,kc_law_date desc
    end
else 
