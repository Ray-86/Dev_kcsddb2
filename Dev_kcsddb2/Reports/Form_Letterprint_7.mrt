		[kcsddb2].[kcsd].[kc_customerloan] as B 
		LEFT JOIN [kcsddb2].[kcsd].[Delegate] L ON B.kc_pusher_code = L.DelegateCode 
		where  
		A.kc_law_date between @pm_strt_date and @pm_stop_date 
		--A.kc_case_no = '2022027'
		and A.kc_case_no = B.kc_case_no
		and A.kc_law_code = '7' 
		--and kc_item_no=@kc_item_no
