		CASE WHEN A.kc_perm_flag = 'Y' THEN B.kc_id_no WHEN A.kc_curr_flag = 'Y' THEN B.kc_id_no WHEN A.kc_comp_flag = 'Y' THEN B.kc_id_no WHEN A.kc_perm_flag1 = 'Y' THEN B.kc_id_no1 WHEN A.kc_curr_flag1 = 'Y' THEN B.kc_id_no1 WHEN A.kc_comp_flag1 = 'Y' THEN B.kc_id_no1 WHEN A.kc_perm_flag2 = 'Y' THEN B.kc_id_no2 WHEN A.kc_curr_flag2 = 'Y' THEN B.kc_id_no2 WHEN A.kc_comp_flag2 = 'Y' THEN B.kc_id_no2 WHEN A.kc_bill_flag = 'Y' THEN B.kc_id_no END AS 收件人ID,
		CASE WHEN A.kc_perm_flag = 'Y' THEN B.kc_perm_zip WHEN A.kc_curr_flag = 'Y' THEN B.kc_curr_zip WHEN A.kc_comp_flag = 'Y' THEN B.kc_comp_zip WHEN A.kc_perm_flag1 = 'Y' THEN B.kc_perm_zip1 WHEN A.kc_curr_flag1 = 'Y' THEN B.kc_curr_zip1 WHEN A.kc_comp_flag1 = 'Y' THEN B.kc_comp_zip1 WHEN A.kc_perm_flag2 = 'Y' THEN B.kc_perm_zip2 WHEN A.kc_curr_flag2 = 'Y' THEN B.kc_curr_zip2 WHEN A.kc_comp_flag2 = 'Y' THEN B.kc_comp_zip2 WHEN A.kc_bill_flag = 'Y' THEN B.kc_zip_code END AS 收件郵遞區號,
