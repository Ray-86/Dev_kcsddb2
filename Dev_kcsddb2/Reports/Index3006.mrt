          <value>wk_issu_code,,22,2</value>
        </Parameters>
        <SqlCommand>select m.CompanyID,m.AllowanceNo,m.AllowanceDate,m.AllowanceType,CONVERT(varchar(1), m.AllowanceStatus) as InvoStatus,m.InvoType,m.CustomerID,m.TotalAmount,m.TaxAmount,m.TotalTaxAmount,c.kc_cust_nameu,Remarks,d.OriginalDescription,d.OriginalInvoiceDate,d.OriginalInvoiceNumber,d.OriginalSequenceNumber,d.AllowanceSequenceNumber,d.Quantity,d.UnitPrice,d.Amount,d.Tax
	from kcsd.InvoAllowanceMaster m left join kcsd.kc_customerloan c On c.kc_case_no = m.CustomerID
                                    left join kcsd.InvoAllowanceDetail d On d.AllowanceNo = m.AllowanceNo and d.InvoType = m.InvoType and d.CompanyID = m.CompanyID
where m.CompanyID = @wk_issu_code and m.AllowanceDate between @wk_strt_date and @wk_stop_date
</SqlCommand>
