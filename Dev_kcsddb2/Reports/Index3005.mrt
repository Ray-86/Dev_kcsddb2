        <Dictionary isRef="1" />
        <Key>117b913940e24f4f96e0dc4b6ea6d85e</Key>
        <Name>DataSource1</Name>
        <NameInSource>Dys</NameInSource>
        <Parameters isList="true" count="3">
          <value>wk_strt_date,,4,0</value>
          <value>wk_stop_date,,4,0</value>
          <value>wk_issu_code,,22,2</value>
        </Parameters>
        <SqlCommand>SELECT l.kc_case_no,c.kc_issu_code,c.kc_cust_nameu, l.kc_invo_date,c.kc_buy_date, Sum(l.kc_pay_fee) AS kc_pay_fee, sum(kc_break_fee) AS kc_break_fee, Sum(l.kc_intr_fee) AS kc_intr_fee, Sum(l.kc_proc_fee) AS kc_proc_fee, Sum(l.kc_pv_amt2) AS kc_pv_amt2, Sum(l.kc_invo_amt2) AS kc_invo_amt2, Sum(l.kc_proc_amt2) AS kc_proc_amt2, Sum(l.kc_pay_fee) AS wk_expt_fee, Sum([kc_invo_amt2]+[kc_proc_amt2]-[kc_intr_fee]) AS kc_local_sum, Max(l.kc_oinvo_amt) AS kc_oinvo_amt
	FROM kcsd.KC_LOANPAYMENT l INNER JOIN kcsd.KC_CUSTOMERLOAN c ON l.kc_case_no = c.kc_case_no
WHERE 
c.kc_issu_code = @wk_issu_code 
and (@wk_issu_code &lt;&gt; '06' or (@wk_issu_code = '06' and kc_prod_type &lt;&gt; '04'))
and
c.kc_prod_type &lt;&gt; '08' AND --勞務件
kc_loan_stat Not In ('X','Y','Z') and
(c.kc_case_no BETWEEN '0' AND '5' OR c.kc_case_no LIKE 'T%' OR c.kc_case_no LIKE 'B%') and
l.kc_invo_date Between @wk_strt_date And @wk_stop_date AND
(c.kc_idle_date Is Null Or c.kc_idle_date &gt;= @wk_stop_date)
GROUP BY l.kc_case_no,c.kc_issu_code,c.kc_cust_nameu,l.kc_invo_date,c.kc_buy_date
HAVING (((Sum([kc_invo_amt2]+[kc_proc_amt2]-[kc_intr_fee]))&lt;0))
</SqlCommand>
      </DataSource1>
