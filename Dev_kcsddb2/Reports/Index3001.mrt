        <SqlCommand>select m.CompanyID,m.InvoNo,m.InvoDate,m.InvoTime,CONVERT(varchar(1), m.InvoStatus) as InvoStatus,m.InvoType,m.CustomerID,m.TaxType,m.SubTotal,m.TaxAmount,m.TotalAmount,c.kc_cust_nameu,m.Remarks
