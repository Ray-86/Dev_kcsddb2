        <CommandTimeout>30</CommandTimeout>
        <Dictionary isRef="1" />
        <Key>f71c5503e0c6489fa799fd75f07f35a1</Key>
        <Name>kct_SimpleCourtList</Name>
        <NameInSource>Dys</NameInSource>
        <Parameters isList="true" count="0" />
        <SqlCommand>
select kc_case_no,SUBSTRING(kc_perm_addr,2,4) kc_perm_addr,SUBSTRING(kc_perm_addr1,2,4) kc_perm_addr1,SUBSTRING(kc_perm_addr2,2,4) kc_perm_addr2,
L0.kc_simplecourt_name ,L1.kc_simplecourt_name as kc_simplecourt_name1 ,L2.kc_simplecourt_name as kc_simplecourt_name2
from kcsd.kc_customerloan c
left join (select SUBSTRING(kc_county_name,2,2) + SUBSTRING(kc_township_name,1,2) as lookup , kc_simplecourt_name from kcsd.kct_SimpleCourtList) L0 on SUBSTRING(c.kc_perm_addr,2,4) = L0.lookup
left join (select SUBSTRING(kc_county_name,2,2) + SUBSTRING(kc_township_name,1,2) as lookup , kc_simplecourt_name from kcsd.kct_SimpleCourtList) L1 on SUBSTRING(c.kc_perm_addr1,2,4) = L1.lookup
left join (select SUBSTRING(kc_county_name,2,2) + SUBSTRING(kc_township_name,1,2) as lookup , kc_simplecourt_name from kcsd.kct_SimpleCourtList) L2 on SUBSTRING(c.kc_perm_addr2,2,4) = L2.lookup


</SqlCommand>
