        <Name>dataSource</Name>
        <NameInSource>Dys</NameInSource>
        <Parameters isList="true" count="6">
          <value>wk_strt_date,,4,0</value>
          <value>wk_stop_date,,4,0</value>
          <value>wk_letter_date,,4,0</value>
          <value>wk_print_cnt1,,8,0</value>
          <value>wk_print_cnt2,,8,0</value>
          <value>kc_issu_code,,23,0</value>
        </Parameters>
        <SqlCommand>SELECT S.*,
--查調年度 01/01~04/30 前年 05/01~12/31 去年
CASE WHEN @wk_letter_date &gt; CAST(YEAR(GETDATE()) as varchar) + '-04-30' 
THEN CAST(YEAR(GETDATE())-1912 as varchar) ELSE CAST(YEAR(GETDATE())-1913 as varchar) END AS 查調年度 
FROM
(
	SELECT
	A.kc_law_date AS 登記日期, A.kc_case_no AS 客戶編號,
	A.kc_law_code AS 催告代碼, A.kc_law_fmt AS 催告格式,
	CASE WHEN A.kc_perm_flag = 'Y' OR A.kc_curr_flag = 'Y' OR A.kc_comp_flag = 'Y' THEN B.kc_cust_nameu ELSE '' END AS 姓名,
	CASE WHEN A.kc_perm_flag = 'Y' OR A.kc_curr_flag = 'Y' OR A.kc_comp_flag = 'Y' THEN B.kc_id_no ELSE '' END AS 身分證號
	FROM kcsd.kc_lawstatus A
	LEFT JOIN kcsd.kc_customerloan B ON A.kc_case_no = B.kc_case_no
	WHERE A.kc_law_date BETWEEN	@wk_strt_date AND @wk_stop_date 
	AND A.kc_law_code = 'G' AND A.kc_law_fmt = 'A7'
	AND (A.kc_perm_flag = 'Y' OR A.kc_curr_flag = 'Y' OR A.kc_comp_flag = 'Y')
	AND (B.kc_issu_code = @kc_issu_code OR @kc_issu_code = '')
	UNION
	SELECT
	A.kc_law_date AS 登記日期, A.kc_case_no AS 客戶編號,
	A.kc_law_code AS 催告代碼, A.kc_law_fmt AS 催告格式,
	CASE WHEN A.kc_perm_flag1 = 'Y' OR A.kc_curr_flag1 = 'Y' OR A.kc_comp_flag1 = 'Y' THEN B.kc_cust_name1u ELSE '' END AS 姓名,
	CASE WHEN A.kc_perm_flag1 = 'Y' OR A.kc_curr_flag1 = 'Y' OR A.kc_comp_flag1 = 'Y' THEN B.kc_id_no1 ELSE '' END AS 身分證號
	FROM kcsd.kc_lawstatus A
	LEFT JOIN kcsd.kc_customerloan B ON A.kc_case_no = B.kc_case_no
	WHERE A.kc_law_date BETWEEN	@wk_strt_date AND @wk_stop_date 
	AND A.kc_law_code = 'G' AND A.kc_law_fmt = 'A7'
	AND (A.kc_perm_flag1 = 'Y' OR A.kc_curr_flag1 = 'Y' OR A.kc_comp_flag1 = 'Y')
	AND (B.kc_issu_code = @kc_issu_code OR @kc_issu_code = '')
	UNION
	SELECT
	A.kc_law_date AS 登記日期, A.kc_case_no AS 客戶編號,
	A.kc_law_code AS 催告代碼, A.kc_law_fmt AS 催告格式,
	CASE WHEN A.kc_perm_flag2 = 'Y' OR A.kc_curr_flag2 = 'Y' OR A.kc_comp_flag2 = 'Y' THEN B.kc_cust_name2u ELSE '' END AS 姓名,
	CASE WHEN A.kc_perm_flag2 = 'Y' OR A.kc_curr_flag2 = 'Y' OR A.kc_comp_flag2 = 'Y' THEN B.kc_id_no2 ELSE '' END AS 身分證號
	FROM kcsd.kc_lawstatus A
	LEFT JOIN kcsd.kc_customerloan B ON A.kc_case_no = B.kc_case_no
	WHERE A.kc_law_date BETWEEN	@wk_strt_date AND @wk_stop_date 
	AND A.kc_law_code = 'G' AND A.kc_law_fmt = 'A7'
	AND (A.kc_perm_flag2 = 'Y' OR A.kc_curr_flag2 = 'Y' OR A.kc_comp_flag2 = 'Y')
	AND (B.kc_issu_code = @kc_issu_code OR @kc_issu_code = '')
) AS S
ORDER BY S.客戶編號
OFFSET (@wk_print_cnt1-1) ROWS
FETCH NEXT (@wk_print_cnt2-@wk_print_cnt1+1) ROWS ONLY
</SqlCommand>
