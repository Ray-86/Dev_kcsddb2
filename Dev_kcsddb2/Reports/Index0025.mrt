        <Name>DataSource</Name>
        <NameInSource>Dys</NameInSource>
        <Parameters isList="true" count="1">
          <value>wk_case_no,,22,7</value>
        </Parameters>
        <SqlCommand>SELECT c.kc_case_no, c.kc_cust_nameu, c.kc_bill_addr, c.kc_zip_code, c.kc_licn_no, c.kc_eng_no, c.kc_new_flag, c.kc_licn_date, ic.kc_issu_desc,
	il.kc_a003, il.kc_a004, il.kc_b003, il.kc_b004, il.kc_c003, il.kc_c004, il.kc_d003, il.kc_d004, il.kc_e003, il.kc_e004, il.kc_f003, il.kc_g003, il.kc_a_type, il.kc_b_type, il.kc_c_type, il.kc_d_type, il.kc_e_type, il.kc_f_type, il.kc_g_type
	FROM kcsd.kc_customerloan c
INNER JOIN kcsd.kc_insurance i ON c.kc_case_no = i.kc_case_no
INNER JOIN kcsd.kct_issuecompany ic ON c.kc_issu_code = ic.kc_issu_code
INNER JOIN (
SELECT kc_case_no, 
	MAX(CASE WHEN kc_insurance_list_insu.kc_insu_cate = 'A003' THEN kct_insucategory.kc_cate_desc + N'承保金額 ' + CONVERT(VARCHAR,kc_insurance_list_insu.kc_insu_amt) + ' 元' ELSE '' END) AS kc_a003,
	MAX(CASE WHEN kc_insurance_list_insu.kc_insu_cate = 'A004' THEN kct_insucategory.kc_cate_desc + N'承保金額 ' + CONVERT(VARCHAR,kc_insurance_list_insu.kc_insu_amt) + ' 元' ELSE '' END) AS kc_a004,
	MAX(CASE WHEN kc_insurance_list_insu.kc_insu_cate = 'B003' THEN kct_insucategory.kc_cate_desc + N'承保金額 ' + CONVERT(VARCHAR,kc_insurance_list_insu.kc_insu_amt) + ' 元' ELSE '' END) AS kc_b003,
	MAX(CASE WHEN kc_insurance_list_insu.kc_insu_cate = 'B004' THEN kct_insucategory.kc_cate_desc + N'承保金額 ' + CONVERT(VARCHAR,kc_insurance_list_insu.kc_insu_amt) + ' 元' ELSE '' END) AS kc_b004,
	MAX(CASE WHEN kc_insurance_list_insu.kc_insu_cate = 'C003' THEN kct_insucategory.kc_cate_desc + N'承保金額 ' + CONVERT(VARCHAR,kc_insurance_list_insu.kc_insu_amt) + ' 元' ELSE '' END) AS kc_c003,
	MAX(CASE WHEN kc_insurance_list_insu.kc_insu_cate = 'C004' THEN kct_insucategory.kc_cate_desc + N'承保金額 ' + CONVERT(VARCHAR,kc_insurance_list_insu.kc_insu_amt) + ' 元' ELSE '' END) AS kc_c004,
	MAX(CASE WHEN kc_insurance_list_insu.kc_insu_cate = 'D003' THEN kct_insucategory.kc_cate_desc + N'承保金額 ' + CONVERT(VARCHAR,kc_insurance_list_insu.kc_insu_amt) + ' 元' ELSE '' END) AS kc_d003,
	MAX(CASE WHEN kc_insurance_list_insu.kc_insu_cate = 'D004' THEN kct_insucategory.kc_cate_desc + N'承保金額 ' + CONVERT(VARCHAR,kc_insurance_list_insu.kc_insu_amt) + ' 元' ELSE '' END) AS kc_d004,
	MAX(CASE WHEN kc_insurance_list_insu.kc_insu_cate = 'E003' THEN kct_insucategory.kc_cate_desc + N'承保金額 ' + CONVERT(VARCHAR,kc_insurance_list_insu.kc_insu_amt) + ' 元' ELSE '' END) AS kc_e003,
	MAX(CASE WHEN kc_insurance_list_insu.kc_insu_cate = 'E004' THEN kct_insucategory.kc_cate_desc + N'承保金額 ' + CONVERT(VARCHAR,kc_insurance_list_insu.kc_insu_amt) + ' 元' ELSE '' END) AS kc_e004,
	MAX(CASE WHEN kc_insurance_list_insu.kc_insu_cate = 'F003' THEN kct_insucategory.kc_cate_desc + N'承保金額 ' + CONVERT(VARCHAR,kc_insurance_list_insu.kc_insu_amt) + ' 元' ELSE '' END) AS kc_f003,
	MAX(CASE WHEN kc_insurance_list_insu.kc_insu_cate = 'G003' THEN kct_insucategory.kc_cate_desc + N'承保金額 ' + CONVERT(VARCHAR,kc_insurance_list_insu.kc_insu_amt) + ' 元' ELSE '' END) AS kc_g003,
	MAX(CASE WHEN kc_insurance_list_insu.kc_insu_cate = 'A003' THEN kc_insu_type ELSE NULL END) AS kc_a_type,
	MAX(CASE WHEN kc_insurance_list_insu.kc_insu_cate = 'B003' THEN kc_insu_type ELSE NULL END) AS kc_b_type,
	MAX(CASE WHEN kc_insurance_list_insu.kc_insu_cate = 'C003' THEN kc_insu_type ELSE NULL END) AS kc_c_type,
	MAX(CASE WHEN kc_insurance_list_insu.kc_insu_cate = 'D003' THEN kc_insu_type ELSE NULL END) AS kc_d_type,
	MAX(CASE WHEN kc_insurance_list_insu.kc_insu_cate = 'E003' THEN kc_insu_type ELSE NULL END) AS kc_e_type,
	MAX(CASE WHEN kc_insurance_list_insu.kc_insu_cate = 'F003' THEN kc_insu_type ELSE NULL END) AS kc_f_type,
	MAX(CASE WHEN kc_insurance_list_insu.kc_insu_cate = 'G003' THEN kc_insu_type ELSE NULL END) AS kc_g_type
	FROM kcsd.kc_insurance_list_insu
	INNER JOIN kcsd.kct_insucategory ON kc_insurance_list_insu.kc_insu_cate = kct_insucategory.kc_insu_cate
	GROUP BY kc_case_no) il ON c.kc_case_no = il.kc_case_no
WHERE c.kc_case_no = @wk_case_no
--WHERE c.kc_case_no = '1800000'</SqlCommand>
