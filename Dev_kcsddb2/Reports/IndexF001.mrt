        <Name>DataSource1</Name>
        <NameInSource>Dys</NameInSource>
        <Parameters isList="true" count="4">
          <value>wk_apply_department,,22,12</value>
          <value>wk_list_user,,22,6</value>
          <value>wk_stop_date,,22,20</value>
          <value>wk_strt_date,,22,20</value>
        </Parameters>
        <SqlCommand>
select case when kc_cancel_user is not null then '已作廢' 
 when kc_itfinish_user is  null then '資訊未完成'
 when kc_itfinish_date &gt;= kc_ittarget_date then '超出7號完成'
 when kc_itfinish_date &lt; kc_ittarget_date then '7號前完成'
else '異常' end IT_type ,

case when kc_cancel_user is not null then '已作廢' 
when kc_test_date is  null then '未完成'
when kc_test_date &gt;= kc_testtarget_date then '超出10號完成'
when kc_test_date &lt; kc_testtarget_date then '10號前完成'
else '異常' end Test_type ,

case when kc_cancel_user is  null  and  kc_itfinish_date &lt; kc_ittarget_date then 1
else 0 end IT_target_cnt ,

case when kc_cancel_user is  null  and  kc_test_date &lt; kc_testtarget_date then 1
else 0 end Test_target_cnt ,


* from

(
select CONVERT(varchar(7), dateadd(mm,1,kc_list_date) , 120) + '-08' as kc_ittarget_date,CONVERT(varchar(7), dateadd(mm,1,kc_list_date) , 120) + '-11' as kc_testtarget_date,*
	from kcsd.kc_itlist where kc_list_date between @wk_strt_date and DATEADD (ss ,-1,DATEADD (dd,1,@wk_stop_date))
and kc_apply_date between @wk_strt_date and DATEADD (ss ,-1,DATEADD (dd,1,@wk_stop_date))
AND (@wk_apply_department = '' OR kc_apply_department = @wk_apply_department)
AND (@wk_list_user = '' OR kc_list_user = @wk_list_user)
) as xx</SqlCommand>
      </DataSource1>
