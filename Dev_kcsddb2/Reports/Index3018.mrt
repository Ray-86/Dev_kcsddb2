          <value>kc_pay_sum,System.Int32</value>
          <value>kc_pay_type,System.String</value>
          <value>kc_proc_sum,System.Int32</value>
          <value>kc_proc_sum2,System.Int32</value>
          <value>kc_pv_sum2,System.Int32</value>
          <value>kc_pvpay_sum2,System.Int32</value>
        </Columns>
        <CommandTimeout>180</CommandTimeout>
        <Dictionary isRef="1" />
        <Key>aafde94724334d1f915287768832f6c1</Key>
        <Name>DataSource1</Name>
        <NameInSource>Dys</NameInSource>
        <Parameters isList="true" count="2">
          <value>wk_year_no,,22,4</value>
          <value>wk_issu_code,,23,2</value>
        </Parameters>
        <SqlCommand>SELECT l.kc_case_no, c.kc_issu_code, c.kc_cust_nameu, l.kc_invo_date, l.kc_pay_type,count(*) as cnt,
	Sum(l.kc_pay_fee) AS kc_pay_sum,
	Sum(kc_break_fee) AS kc_break_sum,
	Sum(l.kc_intr_fee) AS kc_intr_sum,
	Sum(l.kc_proc_fee) AS kc_proc_sum,
	Sum(l.kc_pv_amt2) AS kc_pv_sum2,
	Sum(l.kc_invo_amt2) AS kc_invo_sum2,
	Sum(l.kc_proc_amt2) AS kc_proc_sum2,
	Sum([kc_invo_amt2]+[kc_proc_amt2]-[kc_intr_fee]) AS kc_local_sum,
	case when Sum([kc_invo_amt2]+[kc_proc_amt2]-[kc_intr_fee])&gt; 0 Then Sum([kc_invo_amt2]+[kc_proc_amt2]-[kc_intr_fee]) ELSE 0 END As kc_local_sum2,
		Sum(l.kc_pvpay_amt2) AS kc_pvpay_sum2,
		Max(l.kc_oinvo_amt) AS kc_oinvo_sum
		FROM kcsd.kc_loanpayment l
	INNER JOIN kcsd.kc_customerloan c ON l.kc_case_no = c.kc_case_no
	WHERE c.kc_loan_stat Not In ('X','Y','Z') AND 
	(l.kc_case_no BETWEEN '0' AND '5' OR l.kc_case_no LIKE 'T%' OR l.kc_case_no LIKE 'B%')  AND
	c.kc_issu_code = @wk_issu_code 
and (@wk_issu_code &lt;&gt; '06' or (@wk_issu_code = '06' and kc_prod_type &lt;&gt; '04'))
AND
	c.kc_prod_type &lt;&gt; '08' AND --勞務件
	l.kc_invo_date Between DATEADD(yy, DATEDIFF(yy,0,@wk_year_no), 0)  And DATEADD(ms,-3,DATEADD(yy, DATEDIFF(yy,0,@wk_year_no)+1, 0)) AND
	(c.kc_idle_date Is Null Or c.kc_idle_date &gt;= l.kc_invo_date) 
	GROUP BY l.kc_case_no, c.kc_issu_code, c.kc_cust_nameu, l.kc_invo_date, l.kc_pay_type</SqlCommand>
      </DataSource1>
