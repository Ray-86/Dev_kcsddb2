        </Columns>
        <CommandTimeout>180</CommandTimeout>
        <Dictionary isRef="1" />
        <Key>d777ed241c7d406daf7e8dcf554b8464</Key>
        <Name>DataSource1</Name>
        <NameInSource>Dys</NameInSource>
        <Parameters isList="true" count="3">
          <value>wk_strt_date,,4,0</value>
          <value>wk_stop_date,,4,0</value>
          <value>wk_issu_code,,22,2</value>
        </Parameters>
        <SqlCommand>SELECT l.kc_case_no, c.kc_issu_code, c.kc_cust_nameu, l.kc_invo_date,c.kc_buy_date,
	Sum(l.kc_pay_fee) AS kc_pay_sum,
	Sum(l.kc_break_fee) AS kc_break_sum,
	Sum(l.kc_intr_fee) AS kc_intr_sum,
	Sum(l.kc_proc_fee) AS kc_proc_sum,
	Sum(l.kc_pv_amt2) AS kc_pv_sum2,
	Sum(l.kc_invo_amt2) AS kc_invo_sum2,
	Sum(l.kc_proc_amt2) AS kc_proc_sum2,
	Sum(kc_invo_amt2+kc_proc_amt2-kc_intr_fee) AS kc_local_sum,
	Max(l.kc_oinvo_amt) AS kc_oinvo_sum
	FROM kcsd.kc_loanpayment l
INNER JOIN kcsd.kc_customerloan c ON l.kc_case_no = c.kc_case_no
WHERE c.kc_loan_stat Not In ('X','Y','Z') AND 
(l.kc_case_no BETWEEN '0' AND '5' OR l.kc_case_no LIKE 'T%'OR l.kc_case_no LIKE 'B%')  AND
c.kc_issu_code = @wk_issu_code
and (@wk_issu_code &lt;&gt; '06' or (@wk_issu_code = '06' and kc_prod_type &lt;&gt; '04'))
 AND
c.kc_prod_type &lt;&gt; '08' AND --勞務件
l.kc_invo_date Between @wk_strt_date And @wk_stop_date AND
(c.kc_idle_date Is Null Or c.kc_idle_date &gt;= @wk_stop_date) 
GROUP BY l.kc_case_no, c.kc_issu_code, c.kc_cust_nameu, l.kc_invo_date,c.kc_buy_date
HAVING (CASE WHEN Sum(kc_invo_amt2+kc_proc_amt2-kc_intr_fee) - Max(l.kc_oinvo_amt) &gt; 0 THEN Sum(kc_invo_amt2+kc_proc_amt2-kc_intr_fee) + Max(l.kc_oinvo_amt) ELSE 0 END) + Sum(l.kc_break_fee) &gt;0</SqlCommand>
      </DataSource1>
