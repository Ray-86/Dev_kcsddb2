        <Dictionary isRef="1" />
        <Key>2a6fc473faae43f887ad3d8056f99b67</Key>
        <Name>DataSource1</Name>
        <NameInSource>Dys</NameInSource>
        <Parameters isList="true" count="3">
          <value>wk_strt_date,,4,0</value>
          <value>wk_stop_date,,4,0</value>
          <value>wk_prod_type,,23,0</value>
        </Parameters>
        <SqlCommand>select
	c.kc_case_no,
	c.kc_cust_nameu,
	(select kc_risk_level from kcsd.kct_risklevel where kc_prod_type = p.kc_prod_type and kc_risk_lower &lt; p.kc_risk_score and kc_risk_upper &gt;= p.kc_risk_score) as kc_risk_level,
	p.kc_area_code,
	p.kc_sales_code,
	c.kc_loan_stat,
	c.kc_pusher_code,
	c.kc_comp_code,
	p.kc_cp_date,
(Select CASE WHEN count(p1.kc_case_no) &gt;= 1 THEN 1  ELSE 0 END as cnt from kcsd.kc_pushassign p1 left join kcsd.kc_customerloan c1 on  p1.kc_case_no = c1.kc_case_no where p1.kc_pusher_code like 'L%' and c1.kc_case_no = c.kc_case_no) as y
from kcsd.kc_cpdata p inner join kcsd.kc_customerloan c on c.kc_cp_no = p.kc_cp_no
where (@wk_prod_type = '' OR p.kc_prod_type = @wk_prod_type)
AND c.kc_loan_stat not in ('X','Y','Z')
AND p.kc_risk_score is not null
and c.kc_buy_date between @wk_strt_date and @wk_stop_date</SqlCommand>
      </DataSource1>
