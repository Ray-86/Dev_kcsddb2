        <CommandTimeout>180</CommandTimeout>
        <Dictionary isRef="1" />
        <Name>letter</Name>
        <NameInSource>Dys</NameInSource>
        <Parameters isList="true" count="3">
          <value>wk_strt_date,,4,0</value>
          <value>wk_stop_date,,4,0</value>
          <value>wk_case_no,,23,0</value>
        </Parameters>
        <SqlCommand>SELECT c.*,F.ContactName,F.ContactData,h.kc_detain_date,h.kc_detain_addr,h.kc_auction_date,h.kc_auction_addr,h.kc_guild_code,h.kc_bidder_name,h.kc_bidder_id,h.kc_licn_date as kc_licn_date1
from kcsd.kc_customerloan c
	LEFT JOIN kcsd.kc_appraiseh h on h.kc_case_no = c.kc_case_no
	LEFT JOIN kcsd.Delegate F ON c.kc_pusher_code = F.DelegateCode
WHERE h.kc_appraise_stat = '40'
AND h.kc_licn_date between @wk_strt_date and @wk_stop_date
AND c.kc_case_no = @wk_case_no</SqlCommand>
      </letter>
