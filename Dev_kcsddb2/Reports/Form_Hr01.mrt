      <record Ref="5" type="Stimulsoft.Report.Dictionary.StiSqlSource" isKey="true">
        <Alias>record</Alias>
        <Columns isList="true" count="10">
          <value>kc_clock_date,System.String</value>
          <value>Weekday,System.String</value>
          <value>kc_online,System.String</value>
          <value>kc_onsource,System.String</value>
          <value>kc_offline,System.String</value>
          <value>kc_offsource,System.String</value>
          <value>kc_onname,System.String</value>
          <value>kc_offname,System.String</value>
          <value>kc_list_stat,System.String</value>
          <value>kc_clock_stat,System.String</value>
        </Columns>
        <CommandTimeout>180</CommandTimeout>
        <Dictionary isRef="1" />
        <Name>record</Name>
        <NameInSource>Dys</NameInSource>
        <Parameters isList="true" count="2">
          <value>wk_UserID,,23,0</value>
          <value>wk_clock_date,,4,0</value>
        </Parameters>
        <SqlCommand>select 
	m.*,b.kc_offline,b.kc_offsource,s.Text as kc_onname,x.Text as kc_offname,ISNULL(l.kc_list_stat,'')+ISNULL(o.kc_list_stat,'') as kc_list_stat,z.kc_clock_stat
                                 FROM   
(select CONVERT(VARCHAR(10),DATEADD(DAY, number, @wk_clock_date),111) AS kc_clock_date,SUBSTRING(Datename(Weekday, CONVERT(VARCHAR(10),DATEADD(DAY, number, @wk_clock_date),111)),3,1) as Weekday , a.kc_online,a.kc_onsource
	from
(select convert(varchar, kc_clock_date, 111) as kc_clock_date ,  CONVERT(varchar(100), CreateDate, 24) as kc_online ,kc_clock_source as kc_onsource from [kcsd].[kc_hrattendance]  where  kc_clock_type = '01' and UserID = @wk_UserID) a
                                        right JOIN master..spt_values c on  DATEADD(DAY, number, @wk_clock_date) = a.kc_clock_date
                                        where  type =  'P'and number BETWEEN 0 and  DATEDIFF(DAY,@wk_clock_date, dateadd(ss, -1, dateadd(m, datediff(m, 0, @wk_clock_date) + 1, 0)))) m
						   left join(select convert(varchar, kc_clock_date, 111) as kc_clock_date  , CONVERT(varchar(100), CreateDate, 24) as kc_offline ,kc_clock_source as kc_offsource  from [kcsd].[kc_hrattendance]  where  kc_clock_type = '02'  and UserID = @wk_UserID) b on m.kc_clock_date = b.kc_clock_date
--請假單 單況  
left join
(select CONVERT(VARCHAR(10), kc_list_date,111) AS kc_clock_date ,c.Text + l.kc_hour_cnt + '小時' as kc_list_stat
	from [kcsd].[kc_hrleave] l 
LEFT JOIN (select Value , Text from [Zephyr.Sys].[dbo].[sys_code] where CodeType = 'LeaveType'  ) c ON l.kc_leave_type = c.Value
                                        where UserID = @wk_UserID and kc_hr_user is not null) l on m.kc_clock_date = l.kc_clock_date
--加班單 單況								
left join
(select CONVERT(VARCHAR(10), kc_list_date,111) AS kc_clock_date ,c.Text + o.kc_hour_cnt + '小時' as kc_list_stat
	from [kcsd].[kc_hrover] o 
LEFT JOIN (select Value , Text from [Zephyr.Sys].[dbo].[sys_code] where CodeType = 'OverType'  ) c ON o.kc_over_type = c.Value
                                        where UserID = @wk_UserID and kc_hr_user is not null) o on m.kc_clock_date = o.kc_clock_date
LEFT JOIN (select Value , Text from [Zephyr.Sys].[dbo].[sys_code] where CodeType = 'AttendanceSourceType'  ) s ON m.kc_onsource = s.Value
LEFT JOIN (select Value , Text from [Zephyr.Sys].[dbo].[sys_code] where CodeType = 'AttendanceSourceType'  ) x ON b.kc_offsource = x.Value
-- 欠勤表 打卡狀況
left join
(
select case when h1.kc_clock_date is null then h2.kc_clock_date else  h1.kc_clock_date end as kc_clock_date,case when kc_onstat is null then ISNULL(kc_offstat,'') else kc_onstat +CHAR(10)+ ISNULL(kc_offstat,'') end  as kc_clock_stat from
(select CONVERT(VARCHAR(10), h.kc_clock_date,111) AS kc_clock_date,'上班 : '+ x.Text as kc_onstat from [kcsd].[kc_hrowework] h 
LEFT JOIN (select Value , Text from [Zephyr.Sys].[dbo].[sys_code] where CodeType = 'OweWorkType'  ) x ON h.kc_owe_type = x.Value
where kc_clock_type = '01' and UserID = @wk_UserID and kc_hr_user is not null) h1
full join  
(select CONVERT(VARCHAR(10), h.kc_clock_date,111) AS kc_clock_date, '下班 : '+ x.Text as kc_offstat from [kcsd].[kc_hrowework] h 
LEFT JOIN (select Value , Text from [Zephyr.Sys].[dbo].[sys_code] where CodeType = 'OweWorkType'  ) x ON h.kc_owe_type = x.Value
where kc_clock_type = '02'and UserID = @wk_UserID and kc_hr_user is not null) h2 on h1.kc_clock_date = h2.kc_clock_date
)z on  m.kc_clock_date = z.kc_clock_date
</SqlCommand>
      </record>
