        <Name>DataSource1</Name>
        <NameInSource>Dys</NameInSource>
        <Parameters isList="true" count="3">
          <value>wk_strt_date,,4,0</value>
          <value>wk_stop_date,,4,0</value>
          <value>wk_comp_code,,23,0</value>
        </Parameters>
        <SqlCommand>
select 
	SUBSTRING(c.kc_cp_no ,1,1) + '**' + SUBSTRING(c.kc_cp_no ,4,3) + '**' + SUBSTRING(c.kc_cp_no ,9,2) kc_cp_no,c.kc_cust_nameu,
	SUBSTRING(c.kc_cust_nameu ,1,1) + 'O' + case when LEN(c.kc_cust_nameu) &lt;= 2 then '' when LEN(c.kc_cust_nameu) = 3 then SUBSTRING(c.kc_cust_nameu ,3,1) else 'O' end 	
	+ case when LEN(c.kc_cust_nameu) &lt;= 3 then '' when LEN(c.kc_cust_nameu) = 4 then SUBSTRING(c.kc_cust_nameu ,4,1) else 'O' end 
	+ case when LEN(c.kc_cust_nameu) &lt;= 4 then '' when LEN(c.kc_cust_nameu) = 5 then SUBSTRING(c.kc_cust_nameu ,5,1) else 'O' end 
	+ case when LEN(c.kc_cust_nameu) &lt;= 5 then '' when LEN(c.kc_cust_nameu) = 6 then SUBSTRING(c.kc_cust_nameu ,6,1) else 'O' end 
	+ case when LEN(c.kc_cust_nameu) &lt;= 6 then '' when LEN(c.kc_cust_nameu) = 7 then SUBSTRING(c.kc_cust_nameu ,7,1) else 'O' end 
	+ case when LEN(c.kc_cust_nameu) &lt;= 7 then '' when LEN(c.kc_cust_nameu) = 8 then SUBSTRING(c.kc_cust_nameu ,8,1) else 'O' end kc_cust_nameuA
	,r.*,l.kc_remit_amt as total_kc_remit_amt
	, 
--c.kc_cust_nameu,


case when ISNULL(LEN(a.kc_acc_code),0) &lt; 10 then SUBSTRING(a.kc_acc_code ,1,3) + '******' else 

	SUBSTRING(a.kc_acc_code ,1,3) + '******' + SUBSTRING(a.kc_acc_code ,10, ISNULL(LEN(a.kc_acc_code),0) - 9 ) end kc_acc_code,

c.kc_sales_code,a.kc_agent_code,a.kc_comp_name,a.kc_taxid_no
from kcsd.kc_remittance r
left join kcsd.kc_customerloan c on r.kc_case_no = c.kc_case_no
left join kcsd.kc_caragent a on c.kc_comp_code = a.kc_agent_code
left join kcsd.kc_remittancepaylog l on c.kc_case_no = l.kc_case_no

where 
 r.kc_remit_date between @wk_strt_date and dateadd(ms,-3,DATEADD( DD,1,@wk_stop_date))
and kc_comp_code = @wk_comp_code
and kc_prod_type not in ('04','11')

</SqlCommand>
